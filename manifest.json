{
    "name": "Visual !Bangs",
    "version": "1.2",
    "description": "A convenient method for accessing Google from DuckDuckGo.",
    "permissions": [
        "declarativeContent"
    ],
    "page_action": {
        "default_icon": {
            "48": "data/img/icon48.png",
            "128": "data/img/icon128.png"
        }
    },
    "icons": {
        "48": "data/img/icon48.png",
        "128": "data/img/icon128.png"
    },
    "content_scripts": [
        {
            "js": [
                "data/js/content.js"
            ],
            "matches": [
                "https://duckduckgo.com/*"
            ]
        }
    ],
    "manifest_version": 2
}