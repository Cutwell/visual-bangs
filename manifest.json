{
    "name": "Visual !Bangs",
    "version": "1.3",
    "description": "A convenient method for accessing Google from DuckDuckGo.",
    "action": {
        "default_icon": {
            "48": "data/img/icon48.png",
            "128": "data/img/icon128.png"
        }
    },
    "icons": {
        "48": "data/img/icon48.png",
        "128": "data/img/icon128.png"
    },
    "content_scripts": [
        {
            "js": [
                "data/js/content.js"
            ],
            "css": [
                "data/css/extension.css"
            ],
            "matches": [
                "https://duckduckgo.com/*"
            ]
        }
    ],
    "manifest_version": 3
}